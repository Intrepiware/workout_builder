@model ErrorViewModel;

@{
    ViewData["Title"] = "Error";
}


<section class="section">
    <div class="container is-max-desktop">
        <div class="content">
            <h1>Sorry!</h1>
            <p>An error has occurred while processing your request. The website owners have been notified.</p>
            <p>Trace ID: <code>@Model.RequestId</code></p>
        </div>
    </div>
</section>